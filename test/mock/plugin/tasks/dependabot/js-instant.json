{
  "target": ".dependabot/config.yml",
  "strategy": "overwrite",
  "snippets": [
    {
      "name": "base",
      "variables": {
        "updateSchedule": "live",
        "packageManager": "javascript",
        "targetBranch": "dev"
      }
    }
  ],
  "requires": [
    "dependabot",
    "github",
    "javascript"
  ],
  "purpose": [
    "Configure dependabot to instantly merge javascript dependency updates into the `dev` branch."
  ]
}
