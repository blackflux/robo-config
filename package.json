{
  "name": "robo-config",
  "version": "0.0.0-development",
  "description": "Automatically manage configuration files.",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npx babel src --out-dir lib --copy-files --include-dotfiles --config-file ./.babelrc",
    "build-clean": "npm run clean && npm run build",
    "test-simple": "nyc mocha \"./test/**/*.spec.js\"",
    "test": "npm run clean && npm run gardener && npm run test-simple",
    "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "semantic-release": "npm run build-clean && npx semantic-release",
    "gardener": "node gardener"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blackflux/robo-config.git"
  },
  "keywords": [
    "config",
    "automation",
    "management",
    "configuration"
  ],
  "author": "Lukas Siemon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blackflux/robo-config/issues"
  },
  "homepage": "https://github.com/blackflux/robo-config#readme",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/register": "7.4.4",
    "@blackflux/eslint-plugin-rules": "1.0.19",
    "@blackflux/robo-config-plugin": "1.10.6",
    "babel-eslint": "10.0.2",
    "chai": "4.2.0",
    "coveralls": "3.0.4",
    "eslint": "5.16.0",
    "js-gardener": "2.0.40",
    "nyc": "14.1.1",
    "semantic-release": "15.13.16",
    "tmp": "0.1.0"
  },
  "nyc": {
    "tempDir": "./coverage/.nyc_output",
    "report-dir": "./coverage",
    "check-coverage": true,
    "per-file": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "extension": [],
    "cache": true,
    "all": true,
    "babel": true,
    "exclude": [
      "gardener.js",
      "node_modules/*",
      "coverage/*",
      "lib/*"
    ]
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/blackflux/robo-config/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">= 8.10"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@hapi/joi": "15.0.3",
    "app-root-path": "2.2.1",
    "deepmerge": "3.2.1",
    "joi-strict": "1.0.2",
    "lodash.clonedeep": "4.5.0",
    "lodash.difference": "4.5.0",
    "lodash.get": "4.4.2",
    "object-deep-contain": "1.0.11",
    "object-scan": "10.0.3",
    "object-treeify": "1.1.12",
    "smart-fs": "1.8.0"
  }
}
