{
  "target": ".dependabot/config.yml",
  "strategy": "overwrite",
  "snippets": [
    {
      "name": "base",
      "variables": {
        "updateSchedule": "live",
        "packageManager": "javascript",
        "targetBranch": "dev"
      }
    }
  ],
  "requires": [
    "dependabot",
    "github",
    "javascript"
  ],
  "description": "Dependabot config to instantly merge javascript dependency updated into the dev branch."
}
