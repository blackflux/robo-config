{
  "target": "package.json",
  "strategy": "merge-shallow",
  "snippets": [
    {
      "name": "MIT-package",
      "variables": {
        "repoKey": "${repoKey}",
        "authorName": "${authorName}"
      }
    }
  ],
  "description": "Link MIT license file into [npm](https://www.npmjs.com/) configuration.",
  "requires": [
    "npm"
  ]
}
